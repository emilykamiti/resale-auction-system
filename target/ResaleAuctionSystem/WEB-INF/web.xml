 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
<context-param>
    <param-name>AppName</param-name>
    <param-value> Auction system </param-value>
  </context-param>

  <servlet>
    <servlet-name>LandingAction</servlet-name>
    <servlet-class>com.resale.action.LandingAction</servlet-class>
  </servlet>

    <servlet-mapping>
    <servlet-name>LandingAction</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>